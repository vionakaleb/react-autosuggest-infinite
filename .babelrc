{
  "presets": ["@babel/env", "@babel/preset-react"],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "babel-plugin-transform-react-remove-prop-types", {
            "mode": "wrap"
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        "istanbul",
      ]
    }
  }
}
